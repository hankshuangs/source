    <UserName>administrator</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="EmployeesContext" Order="1" Enabled="False">
          <Destination Path="Data Source=.;Initial Catalog=EmployeesContext;Persist Security Info=True;User ID=sa;Password=missfor2006;Connect Timeout=30" />
