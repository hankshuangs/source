    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>IBUS-168-52</MSDeployServiceURL>
    <DeployIisAppPath>WebServices</DeployIisAppPath>
    <RemoteSitePhysicalPath />
